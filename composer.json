{
    "name": "tonicospinelli/classgeneration",
    "description": "Library to create Php Class files",
    "homepage": "https://github.com/tonicospinelli/ClassGeneration/",
    "type": "library",
    "keywords": [
        "php class generator",
        "php class gen",
        "php class",
        "class gen",
        "class generation",
        "class generator",
        "php gen",
        "php generator"
    ],
    "authors": [
        {
            "name": "Antonio Spinelli",
            "email": "tonicospinelli85@gmail.com",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/tonicospinelli/ClassGeneration/issues"
    },
    "require": {
        "php": ">=5.3.0",
        "ext-reflection": "*",
        "ext-spl": "*"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "satooshi/php-coveralls": "dev-master",
        "phpunit/phpunit": "3.7@stable",
        "pdepend/pdepend": "*",
        "sebastian/phpcpd": "*",
        "squizlabs/php_codesniffer": "*",
        "phpmd/phpmd": "*"
    },
    "autoload": {
        "psr-0": { "ClassGeneration": "src/"}
    }
}
